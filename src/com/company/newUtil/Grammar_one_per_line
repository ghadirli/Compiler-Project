PROGRAM -> DECLARATIONLIST eof
DECLARATIONLIST -> DECLARATION DECLARATIONLIST
DECLARATIONLIST -> epsilon
DECLARATION -> void id DECLARATION2
DECLARATION -> int id DECLARATION2
DECLARATION2 -> VARDECLARATION2
DECLARATION2 -> ( PARAMS ) COMPOUNDSTMT
VARDECLARATION2 -> ;
VARDECLARATION2 -> bopen num bclose ;
PARAMS -> int id PARAM2 PARAMLIST2
PARAMS -> void PARAMS2
PARAMS2 -> id PARAM2 PARAMLIST2
PARAMS2 -> epsilon
PARAMLIST2 -> comma PARAMLIST3
PARAMLIST2 -> epsilon
PARAMLIST3 -> int id PARAM2 PARAMLIST2
PARAMLIST3 -> void id PARAM2 PARAMLIST2
PARAM2 -> bopen bclose
PARAM2 -> epsilon
COMPOUNDSTMT -> aopen DECLARATIONLIST STATEMENTLIST aclose
STATEMENTLIST -> STATEMENT STATEMENTLIST
STATEMENTLIST -> epsilon
STATEMENT -> EXPRESSIONSTMT
STATEMENT -> COMPOUNDSTMT
STATEMENT -> SELECTIONSTMT
STATEMENT -> ITERATIONSTMT
STATEMENT -> RETURNSTMT
STATEMENT -> SWITCHSTMT
EXPRESSIONSTMT -> EXPRESSION ;
EXPRESSIONSTMT -> continue ;
EXPRESSIONSTMT -> break ;
EXPRESSIONSTMT -> ;
SELECTIONSTMT -> if ( EXPRESSION ) STATEMENT else STATEMENT
ITERATIONSTMT -> while ( EXPRESSION ) STATEMENT
RETURNSTMT -> return RETURNSTMT2
RETURNSTMT2 -> ;
RETURNSTMT2 -> EXPRESSION ;
SWITCHSTMT -> switch ( EXPRESSION ) aopen CASESTMTS DEFAULTSTMT aclose
CASESTMTS -> CASESTMT CASESTMTS
CASESTMTS -> epsilon
CASESTMT -> case num : STATEMENTLIST
DEFAULTSTMT -> default : STATEMENTLIST
DEFAULTSTMT -> epsilon
EXPRESSION -> id EXPRESSION2
EXPRESSION -> num TERM2 ADDITIVEEXPRESSION2 SIMPLEEXPRESSION2
EXPRESSION -> ( EXPRESSION ) TERM2 ADDITIVEEXPRESSION2 SIMPLEEXPRESSION2
EXPRESSION -> + FACTOR TERM2 ADDITIVEEXPRESSION2 SIMPLEEXPRESSION2
EXPRESSION -> minus FACTOR TERM2 ADDITIVEEXPRESSION2 SIMPLEEXPRESSION2
EXPRESSION2 -> VAR2 EXPRESSION3
EXPRESSION2 -> ( ARGS ) TERM2 ADDITIVEEXPRESSION2 SIMPLEEXPRESSION2
EXPRESSION3 -> equal EXPRESSION
EXPRESSION3 -> TERM2 ADDITIVEEXPRESSION2 SIMPLEEXPRESSION2
VAR2 -> bopen EXPRESSION bclose
VAR2 -> epsilon
SIMPLEEXPRESSION2 -> RELOP SIGNEDFACTOR TERM2 ADDITIVEEXPRESSION2
SIMPLEEXPRESSION2 -> epsilon
RELOP -> less
RELOP -> equalequal
ADDITIVEEXPRESSION2 -> ADDOP SIGNEDFACTOR TERM2 ADDITIVEEXPRESSION2
ADDITIVEEXPRESSION2 -> epsilon
ADDOP -> +
ADDOP -> minus
TERM2 -> * SIGNEDFACTOR TERM2
TERM2 -> epsilon
SIGNEDFACTOR -> FACTOR
SIGNEDFACTOR -> + FACTOR
SIGNEDFACTOR -> minus FACTOR
FACTOR -> ( EXPRESSION )
FACTOR -> id FACTOR2
FACTOR -> num
FACTOR2 -> VAR2
FACTOR2 -> ( ARGS )
ARGS -> ARGLIST
ARGS -> epsilon
ARGLIST -> EXPRESSION ARGLIST2
ARGLIST2 -> comma EXPRESSION ARGLIST2
ARGLIST2 -> epsilon


/* official grammar */